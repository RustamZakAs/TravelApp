<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TravelApp"
                    xmlns:v="clr-namespace:TravelApp.Views"
                    xmlns:vm="clr-namespace:TravelApp.ViewModels">
    <DataTemplate DataType="{x:Type vm:LogInViewModel}">
        <v:LogInView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:MenyuViewModel}">
        <v:MenyuView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TripsViewModel}">
        <v:TripsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:RegistrationViewModel}">
        <v:RegistrationView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:WeatherViewModel}">
        <v:WeatherView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CitiesViewModel}">
        <v:CitiesView/>
    </DataTemplate>
</ResourceDictionary>